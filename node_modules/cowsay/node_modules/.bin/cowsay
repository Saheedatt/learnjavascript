#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/HP/Desktop/Lab js/node_modules/.pnpm/cowsay@1.6.0/node_modules/cowsay/node_modules:/mnt/c/Users/HP/Desktop/Lab js/node_modules/.pnpm/cowsay@1.6.0/node_modules:/mnt/c/Users/HP/Desktop/Lab js/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/HP/Desktop/Lab js/node_modules/.pnpm/cowsay@1.6.0/node_modules/cowsay/node_modules:/mnt/c/Users/HP/Desktop/Lab js/node_modules/.pnpm/cowsay@1.6.0/node_modules:/mnt/c/Users/HP/Desktop/Lab js/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
